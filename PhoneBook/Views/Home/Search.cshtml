@model List<Person> 

@{
    ViewData["Title"] = "Search";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div dir="rtl" >
<div class="row">
@{
    foreach(var item in Model)
    {   

            <div class="col-md-10 ">
                <ul class=" list-unstyled">
                    <li>
                    <ul class="d-flex list-unstyled">
                    <li class="namefam text-primary"><h4>@item.Family</h4></li>
                    <li class=" text-primary"><h4>@item.Name</h4></li>
                    </ul>
                    </li>
                    <li  class="align">@item.PhoneNumber</li>
                    <li  class="align">@item.Address</li>
                </ul>
            </div>

    }
}
</div>
</div>
